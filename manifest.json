{
  "name": "Google Contacts2",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self'; default-src 'self' https://ilgazer.github.io",
  "content_scripts": [
    {
      "matches": [
        "*://web.whatsapp.com/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "frame.html"
  ]
}
